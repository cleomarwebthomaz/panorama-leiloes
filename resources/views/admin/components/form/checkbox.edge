<div class="form-group clearfix">
  <div class="icheck-primary d-inline">
    <input 
    	type="checkbox"
      value="{{ value }}"
    	name="{{ name }}"
    	id="{{ id }}"
    	@if(checked)
    		checked="checked" 
    	@endif
      @if(values.includes(value))
        checked="checked" 
      @endif      
    >
    <label for="{{ id }}">
      {{ label }}
    </label>
  </div>
</div>